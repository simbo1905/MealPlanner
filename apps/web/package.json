{
  "name": "@mealplanner/web",
  "version": "0.1.0",
  "private": true,
  "description": "MealPlanner web application built with SvelteKit",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3001",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "svelte-check --tsconfig ./tsconfig.json"
  },
  "dependencies": {
    "svelte": "^5.5.4",
    "svelte-dnd-action": "^0.9.65",
    "date-fns": "^3.6.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.8.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/vite": "^4.1.14",
    "@types/node": "^22.9.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.14.0",
    "postcss": "^8.5.6",
    "svelte-check": "^4.0.7",
    "tailwindcss": "^4.1.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  }
}
